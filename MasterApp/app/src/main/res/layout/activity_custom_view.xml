<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.haitao.www.myformer.ui.ui_common.component.customassemblewidget.CustomDescribeTitle
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:describe_title="你好"
        app:describe_title_color="@color/blueviolet" />

    <com.haitao.www.myformer.ui.ui_common.component.customassemblewidget.CustomTitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:right_button_drawable="@drawable/ic_delete"
        app:title_background_color="@color/darkcyan"
        app:title_text="你好"
        app:title_text_color="@color/red" />

    <com.haitao.www.myformer.ui.ui_common.component.customassemblewidget.TitleBar
        android:id="@+id/titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>
