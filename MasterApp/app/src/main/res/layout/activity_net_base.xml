<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.haitao.www.myformer.ui.ui_common.component.customassemblewidget.CustomDescribeTitle
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:describe_title="使用InetAddress类获取IP地址" />

    <TextView
        android:id="@+id/tv_local_ip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="本机IP地址：" />

    <TextView
        android:id="@+id/tv_remote_ip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="远程IP地址（百度）：" />
    <TextView
        android:id="@+id/tv_reachable_ip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="本机是否可达：" />



</LinearLayout>
