<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/button_send_notification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="生成通知栏消息"/>

    <Button
        android:id="@+id/button_send_notification02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="生成通知栏消息02"/>

    <Button
        android:id="@+id/button_send_notification03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="发送广播"/>

    <Button
        android:id="@+id/button_send_notification04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拉起另一个App"/>

    <Button
        android:id="@+id/button_send_notification05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="通知栏消息拉起另一个App"/>

</LinearLayout>
