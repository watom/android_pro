<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="4dp"
        android:top="6dp">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/red" />
            <size
                android:width="80dp"
                android:height="70dp" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item
        android:bottom="6dp"
        android:right="3dp">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/greenyellow" />
            <size
                android:width="70dp"
                android:height="80dp" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:left="6dp">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/blue" />
            <size
                android:width="80dp"
                android:height="80dp" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</layer-list>